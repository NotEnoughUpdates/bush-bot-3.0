{
	"configurations": [
		{
			"type": "node-terminal",
			"command": "yarn dev",
			"name": "Tanzanite",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"sourceMaps": true,
			"resolveSourceMapLocations": ["${workspaceFolder}/**"],
			"skipFiles": ["<node_internals>/**", "/**/.yarn/**", "/**/.nvm/**"]
		}
	]
}
